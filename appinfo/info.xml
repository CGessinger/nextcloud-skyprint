    <id>skyprint</id>
    <name>SkyPrint</name>
    <summary>Print documents directly from your cloud-storage</summary>
    <description><![CDATA[A straightforward app enabling users to easily print documents directly from your cloud storage. With Sky Print, you can adjust a variety of print settings, such as paper size, orientation and quality.]]></description>
    <version>0.1.1</version>
    <licence>agpl</licence>
    <author mail="christian@gessinger.de">Christian Gessinger</author>
    <website>https://github.com/CGessinger/nextcloud-skyprint</website>
    <bugs>https://github.com/CGessinger/nextcloud-skyprint/issues</bugs>
    <screenshot>https://raw.githubusercontent.com/CGessinger/nextcloud-skyprint/master/screenshots/interface_v0_1_0.png</screenshot>
    <namespace>SkyPrint</namespace>
    <category>files</category>
    <bugs>https://github.com/CGessinger</bugs>
    <dependencies>
        <nextcloud min-version="25" max-version="27"/>
    </dependencies>
    <settings>
        <admin>OCA\SkyPrint\Settings\SkyPrintAdmin</admin>
        <admin-section>OCA\SkyPrint\Sections\SkyPrintAdmin</admin-section>
    </settings>
</info>
